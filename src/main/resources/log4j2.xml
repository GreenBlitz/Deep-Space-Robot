<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5p %c{1} - %msg%n"/>
        </Console>
    </Appenders>
    <loggers>
        <Root level="trace">
            <AppenderRef ref="Console" level="off"/>
        </Root>
    </loggers>
</Configuration>